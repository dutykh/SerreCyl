}
@article{Batic2024,
   abstract = {<p>We present a comprehensive analysis of quasinormal modes (QNMs) for noncommutative geometry-inspired Schwarzschild black holes, encompassing both non-extreme and extreme cases. By employing a spectral method, we calculate the QNMs in the context of scalar, electromagnetic, and gravitational perturbations. Our findings not only challenge previous claims in the literature regarding the instability of these black holes but also reveal remarkable stability for both non-extreme and extreme Schwarzschild black holes under various perturbations.</p>},
