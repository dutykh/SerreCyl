@article{Dutykh2019,
   abstract = {In the present study we consider three two-component (integrable and non-integrable) systems which describe the propagation of shallow water waves on a constant shear current. Namely, we consider the two-component Camassa-Holm equations, the Zakharov-Ito system and the Kaup-Boussinesq equations all including constant vorticity effects. We analyze both solitary and periodic-type travelling waves using the simple and geometrically intuitive phase space analysis. We get the pulse-type solitary wave solutions and the front solitary wave solutions. For the Zakharov-Ito system we underline the occurrence of the pulse and anti-pulse solutions. The front wave solutions decay algebraically in the far field. For the Kaup-Boussinesq system, interesting analytical multi-pulsed travelling wave solutions are found.},
   author = {D. Dutykh and D. Ionescu-Kruse},
   doi = {10.3934/dcds.2019225},
   issue = {9},
   journal = {Discrete & Continuous Dynamical Systems - A},
   keywords = {Camassa-Holm equations,Kaup-Boussinesq equations,Zakharov-Ito system,analytical solutions,cnoidal waves,multi-pulsed solutions,phase-plane analysis,solitary waves,waves on shear flow},
   pages = {5521-5541},
   title = {Effects of vorticity on the travelling waves of some shallow water two-component systems},
   volume = {39},
   year = {2019}
}
@article{Pham2005,
   abstract = {The stability of perfect-fluid capillary-gravity surface flows past a cylindrical obstacle is studied in the shallow water limit, using the two-dimensional compressible Euler equations, with leading-order dispersive corrections. Stationary solutions with different contact angles are obtained by Newton branch following, based on Fourier pseudospectral methods, using mapped Chebychev polynomials. Stable and unstable branches are found to meet, through a saddle-node bifurcation, at a critical speed beyond which no stationary solution exists. For large obstacles, the stable branch is compared with the stationary solutions of the compressible Euler equation without dispersion. Boundary layers are investigated. In this regime, the unstable dynamics are shown to lead to a finite-time dewetting singularity.},
   author = {C.-T. Pham and C. Nore and M.-E. Brachet},
   doi = {10.1063/1.1926267},
   issn = {1070-6631},
   issue = {6},
   journal = {Phys. Fluids},
   month = {6},
   pages = {062104},
   title = {Critical speed for capillary-gravity surface flows in the dispersive shallow water limit},
   volume = {17},
   url = {http://aip.scitation.org/doi/10.1063/1.1926267},
   year = {2005}
}
@article{Dias1993,
   abstract = {Capillary-gravity solitary waves with damped oscillations are studied analytically. The analysis follows the work of Iooss and Kirchgässner who proved that these waves exist for all values of the Froude number smaller than one. The water-wave problem is reduced to a system of ordinary differential equations by using the center manifold theorem. The normal form of this reduced system can be obtained and a good approximation to these waves for small amplitude is constructed. The limit as the water depth becomes infinite is considered as a special case. A comparison with existing numerical results is made for small-amplitude waves.},
