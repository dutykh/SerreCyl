@inproceedings{Gisclon2017,
   abstract = {Nous présentons un modèle de transfert thermique à travers un film liquide s'écoulant le long d'une paroi verticale.Ce nouveau modèle est formellement obtenu à partir d'un dévelopement asymptotique en introduisant des variables adimensionnées bien choisies. Dans notre étude, le petit paramètre est le rapport épaisseur sur longueur caractéristique des ondes. Une nouvelle solution de Nusselt est obtenue en prenant en compte les variations hydrodynamqiues de la surface libre et les contributions des termes provenant des effets de variations de la température. Les transferts de fluide et de chaleur sont couplés à travers l'effet de Marangoni et des effets de viscosité. Même si ces efffets ont été considérés avant séparément, un modèle couplé est présenté ici. Une autre nouveauté consiste en l'approche asymptotique en comparaison avec l'approche des résidus pondérés qui a été appliqué avant à ces problèmes. Des comparaisons numériques seront présentées également.},
   author = {M. Gisclon and M. Chhay and D. Dutykh and C. Ruyer-Quil},
   city = {Lille, France},
   booktitle = {Congrès Français de Mécanique},
   publisher = {Association Française de Mécanique},
   title = {Modélisation mathématique des films minces avec applications aux transferts de chaleur},
   url = {http://hdl.handle.net/2042/63415},
   year = {2017}
}
@inproceedings{Chhay2015,
   abstract = {Les films liquides tombants représentent un phénomène hydrodynamique à la fois complexe et important pour certaines applications industrielles. Physiquement cela représente une couche liquide mince qui s'écoule le long d'un plan. Mathématiquement, cette situation est décrite par les équations de Navier-Stokes incompressibles en prenant en compte la faible épaisseur de l'écoulement. On s'intéresse donc à un modèle mathématique approché incluant la partie thermique. L'objet de cet exposé est à partir des équations de Navier-Stokes incompressibles de présenter l'adimensionement avec l'introduction des nombres de Peclet, Prandtl, Reynolds, la dérivation formelle du modèle qui donne un système à surface libre hyperbolique et enfin d'obtenir une équation approchée pour le champ de température moyen avec le terme de Nusselt.},
   author = {M. Chhay and D. Dutykh and M. Gisclon and C. Ruyer-Quil},
   city = {Lyon, France},
   booktitle = {Congrès Français de Mécanique},
   publisher = {Association Française de Mécanique},
   title = {Modélisation mathématiques des films minces},
