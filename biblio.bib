   year = {2002}
}
@article{Budd2009,
   abstract = {This article considers a new method for generating a moving mesh which is suitable for the numerical solution of partial differential equations (PDEs) in several spatial dimensions. The mesh is obtained by taking the gradient of a (scalar) mesh potential function which satisfies an appropriate nonlinear parabolic partial differential equation. This method gives a new technique for performing r-adaptivity based on ideas from optimal transportation combined with the equidistribution principle applied to a (time-varying) scalar monitor function (used successfully in moving mesh methods in one-dimension). Detailed analysis of this new method is presented in which the convergence, regularity, and stability of the mesh is studied. Additionally, this new method is shown to be straightforward to program and implement, requiring the solution of only one simple scalar time-dependent equation in arbitrary dimension, with adaptivity along the boundaries handled automatically. We discuss three preexisting methods in the context of this work. Examples are presented in which either the monitor function is prescribed in advance, or it is given by the solution of a partial differential equation.},
   author = {C. J. Budd and J. F. Williams},
   doi = {10.1137/080716773},
   issn = {1064-8275},
   issue = {5},
   journal = {SIAM J. Sci. Comput.},
   month = {1},
   pages = {3438-3465},
   title = {Moving Mesh Generation Using the Parabolic Monge-Ampère Equation},
   volume = {31},
   url = {http://epubs.siam.org/doi/abs/10.1137/080716773},
   year = {2009}
