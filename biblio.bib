@inproceedings{Colinart2017,
   abstract = {Excessive levels of moisture in buildings lead to building pathologies. To highlight the risk of occurrence of each type of pathology, numerous durability indexes and associated models were defined in the literature. These models are based on the knowledge of the relative humidity, the moisture content and/or the temperature level. In the case of hygroscopic building materials, the hygrothermal fields are evaluated by solving heat, air and moisture transfer equations. In this work, we aim to analyze two mould prediction indexes and models frequently used in the building physics field (Mould index M and VTT model, TOW and Sedlbauer’s isopleths). The results are first discussed regarding the anisotropy of transport properties and the hysteresis of the sorption isotherms and then compared to recent laboratory results obtained on wooden materials found in the literature. The observed difference between simulation and experimental data allows highlighting the necessity of either upgrading of the mould growth models or developing new accurate experiments.},
   author = {T. Colinart and M. Bendouma and P. Glouannec},
   city = {Gent, Belgium},
   booktitle = {14th International Conference on Durability of Building Materials and Components},
   title = {Analysis of mould growth models and influence of the hygrothermal properties},
   year = {2017}
}
