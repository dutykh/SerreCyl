@article{Beck2016,
   abstract = {Computer simulators can be computationally intensive to run over a large number of input values, as required for optimization and various uncertainty quantification tasks. The standard paradigm for the design and analysis of computer experiments is to employ Gaussian random fields to model computer simulators. Gaussian process models are trained on input-output data obtained from simulation runs at various input values. Following this approach, we propose a sequential design algorithm MICE (mutual information for computer experiments) that adaptively selects the input values at which to run the computer simulator in order to maximize the expected information gain (mutual information) over the input space. The superior computational efficiency of the MICE algorithm compared to other algorithms is demonstrated by test functions and by a tsunami simulator with overall gains of up to 20% in that case.},
   author = {J. Beck and S. Guillas},
   doi = {10.1137/140989613},
   issn = {2166-2525},
   issue = {1},
   journal = {SIAM/ASA J. Uncertainty Quantification},
   month = {1},
   pages = {739-766},
   title = {Sequential Design with Mutual Information for Computer Experiments (MICE): Emulation of a Tsunami Model},
   volume = {4},
   url = {http://epubs.siam.org/doi/10.1137/140989613},
   year = {2016}
}
@article{Wei2008,
   abstract = {[1] At 23:41 UTC on 15 August 2007, an offshore earthquake of magnitude 8.0 severely damaged central Peru and generated a tsunami. Severe shaking by the earthquake collapsed buildings throughout the region and caused 514 fatalities. The tsunami resulted in three casualties and a representative maximum runup height of ~7 m in the near field. The first real-time tsunami data available came from a deep-ocean tsunami detection buoy within 1 hour of tsunami generation. These tsunami data were used to produce initial experimental forecasts within 2 hours of tsunami generation. The far-field forecasts indicated that the tsunami would not flood any of the 14 U.S. communities. Comparison with real-time tide gage data showed very accurate forecasts.},
   author = {Y. Wei and E. N. Bernard and L. Tang and R. Weiss and V. V. Titov and C. Moore and M. Spillane and M. Hopkins and U. Kanoglu},
